{
  "name": "notejs",
  "version": "0.0.0",
  "dependencies": {
    "finalhandler": "^0.3.1",
    "serve-static": "^1.7.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify": "^6.1.0",
    "browserify-shim": "^3.8.0",
    "emberfire": "^1.2.7",
    "firebase": "^1.1.2",
    "gulp": "^3.8.8",
    "gulp-sourcemaps": "^1.2.4",
    "gulp-uglify": "^1.0.1",
    "handlebars": "1.3.0",
    "jquery": "^2.1.1",
    "moment": "^2.8.3",
    "pagedown": "^1.1.0",
    "randomcolor": "^0.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "build": "browserify public/js/main.js -o public/js/app.js",
    "watch": "watchify public/js/main.js -o public/js/app.js --debug --verbose"
  },
  "browserify": {
    "transform": "browserify-shim"
  },
  "browser": {
    "ember": "./bower_components/ember/ember.js",
    "ember-data": "./bower_components/ember-data/ember-data.min.js"
  },
  "browserify-shim": {
    "ember": {
      "depends": [
        "jquery:jQuery",
        "handlebars:Handlebars"
      ],
      "exports": "Ember"
    },
    "ember-data": {
      "exports": "DS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kentor/notejs.git"
  }
}
